tempF = 70

def Kelvin(tempF):
    K = (tempF-32)*(5/9)+273
    return K
    
if Kelvin(tempF) > 300:
    print(
        tempF, 'degF is',round(Kelvin(tempF)),'Kelvin\n', 'Dang that is hot!'
    );
else:
    print(
       tempF, 'degF is', round(Kelvin(tempF)),'Kelvin\n', 'It is lovely weather wes are having'
    )
